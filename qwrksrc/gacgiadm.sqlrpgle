                   hsrch_type  = 'STG';
                   hcntr_dp    = 'C';
                   panel       = '#DP_SCR1  ';
                   hscrn_next  = 'DP1';

                   // --Diploma Order - Monthly Reports
                when h_action = 'DRP';
                   h_applid    = 'DP';
                   panel       = '#REPORTS  ';
                   hscrn_next  = 'REP';

                   // --Certification Orders - Screen 1
                when h_action = 'CE1';
                   h_applid    = 'CE';
                   hsrch_type  = 'STG';
                   hcntr_dp    = 'O';
                   exsr $GA_Check;
