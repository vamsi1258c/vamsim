          //         MS is the we Mail a Permit out again method
          idx = %scan('((REI)':htmlstmt:1);
          if idx         > 0;
             if grplc_meth  = 'M' and
                    ttempperm#  = *blanks and
                    tsource    <> 'OTC';
                byte6       = 'MS_REI';
             elseif gapplid     = 'MS';
                byte6       = 'MS_REI';
             else;
                byte6       = 'PP_REI';
