                else;

                   dow *in95 <> *on;
                      reade (krecord_id : hveh_make) model_rec;
                      *in95 = %eof;
                      if *in95;
                         leave;
                      endif;

                      clear field_150;
                      if hveh_model  = mvehmodel;
                         field_150   = '<option value="' +
