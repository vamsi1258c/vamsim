       // ***********************************************************************
       begsr PP_CE_UPD;

          clear check_stage;

          //    chain to gaorder1# to update record
          if NOT %open(gaorder1#);
             open(e) gaorder1#;
             *in97 = %error;
          endif;

          chain horder# garec_#;
