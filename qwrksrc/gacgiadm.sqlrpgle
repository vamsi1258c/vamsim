       if hsha_hash <> *blanks;
          exsr Get_Schnam;
          exsr check_sha;
       elseif h_action   <> 'OFF';
          exsr CheckSign;
       endif;
       if condition   <> *blanks;
          panel       = '#LOGONGA ';
          exsr WrtPanel;

     c*                  goto      End
          leavesr;
        endif;


