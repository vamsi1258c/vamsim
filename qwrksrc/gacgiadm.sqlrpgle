                   endif;

                   BufOut = %trimr(BufOut) + field_150;
                   BufOut = %trimr(BufOut) + linefeed;
                   bytesize    = 1 + bytesize +
                                     %len(%trimr(field_150));
                   exsr checkbufr;
                enddo;

                //                          Rabies Month    <SELECT> 1-12 Months
             when slct_type   = 'RMM';

                //   Start with empty option
                clear field_150;
                field_150   = '<option value="' +
                                  '  ">' +
                                  'Month';
                BufOut = %trimr(BufOut) + field_150;
                BufOut = %trimr(BufOut) + linefeed;
                bytesize    = 1 + bytesize +
                                  %len(%trimr(field_150));
                exsr checkbufr;

                for i1 = 1 To 12;
                   clear field_150;
