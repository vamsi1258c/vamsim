          elseif saction     = 'H';
             comp_msg    = 'Hold Change';
          elseif saction     = 'L';
             comp_msg    = 'LOST/STOLEN';
          elseif saction     = 'M';
             comp_msg    = 'Mail Returned     ';
          elseif saction     = 'O';
             comp_msg    = 'Order Receipt';
          elseif saction     = 'P';
             comp_msg    = 'Problem Notification';
          elseif saction     = 'R';
             comp_msg    = 'Re-Issued';
          elseif saction     = 'S';
             comp_msg    = 'Extended Temp';
          elseif saction     = 'X';
