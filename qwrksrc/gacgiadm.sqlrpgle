             h_action    = 'MNU';
             clear hscrn_next;
             panel       = '#RSS_MENU';
             exsr WrtPanel;

             // ---------------------------------------------------------------------
             //     Update the FICE with the NUFICE
             // ---------------------------------------------------------------------
          when h_action = 'NUF';
             setll hnufice instrec;
             *in90 = not %found;
             if NOT %equal;
                hfice       = '009998';
                exsr get_schnam;
             endif;

             hscrn_next  = 'MNU';
             panel       = '#MENU     ';
             exsr WrtPanel;

             // ------------------------------------------------------------------
             //   Action Codes that can occur on any screen are processed first
             // ------------------------------------------------------------------
             // *   OFF action code (must always be first) - Log off the user
          when h_action = 'OFF';

