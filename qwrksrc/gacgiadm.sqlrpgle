                   tga_paidby  = '2';
                endif;
                check_stage = 'Y';
             endif;

             //    UnCheck Paid - Allowed only for CODs
             //    Not allowed because of setting of billlcyle!!!!!!!!
             if hga_paidcd  = ' ';
                //                   clear                   tga_paid
                //                   clear                   tga_paidus
                //                   clear                   tga_paiddt
                //                   clear                   tga_paidtm
             endif;
          endif;

          //    Check Stage if we should advance it
          if check_stage = 'Y';
             if tstage      = *blanks;
