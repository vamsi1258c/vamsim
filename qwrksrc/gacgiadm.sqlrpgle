             evalr bpin# = %editc(cycle_time:'X');

             bchguser    = huserid;
             bchgdate    = cycle_date;
             bchgtime    = cycle_time;

             write ga_busrec;

             ei += 1;
             errs(ei) = 'Business Added +
                             Successfully.';
