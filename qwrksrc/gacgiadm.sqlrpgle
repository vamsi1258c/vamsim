                exsr checkbufr;
                // ----

                setll (krecord_id : kiso_cc) ciccstat1;
                *in95 = not %found;

                dow *in95 <> *on;
                   reade (krecord_id : kiso_cc) ccstate;
                   *in95 = %eof;
                   if *in95;
                      leave;
                   endif;

                   //   Skip if APO in the Name
                   if %scan('APO':eshrtname:1) > 0;
                      iter;
                   endif;

                   clear field_150;
                   if the_state   = estate;
                      field_150   = '<option value="' +
